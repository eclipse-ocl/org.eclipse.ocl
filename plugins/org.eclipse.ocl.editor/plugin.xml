<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.ocl.library.presentation.LibraryModelWizardID"
            name="%_UI_LibraryModelWizard_label"
            class="org.eclipse.ocl.library.presentation.OCLLibraryModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/LibraryModelFile.gif">
         <description>%_UI_LibraryModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.ocl.library.presentation.OCLLibraryEditor"
            contributorClass="org.eclipse.ocl.library.presentation.OCLLibraryActionBarContributor"
            extensions="oclstdlib"
            icon="icons/full/obj16/LibraryModelFile.gif"
            id="org.eclipse.ocl.library.presentation.LibraryEditorID"
            name="%_UI_LibraryEditor_label">
         <contentTypeBinding contentTypeId="org.eclipse.ocl.library"/>
      </editor>
   </extension>

</plugin>
