<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="env" nsURI="http://www.example.org/examples/env.ecore" nsPrefix="env">
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
    <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="Environment">
    <eOperations name="addElement" lowerBound="1" eType="#//Environment">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="body" value="Environment{ elements = self.elements->including(element) }"/>
      </eAnnotations>
      <eParameters name="element" lowerBound="1" eType="ecore:EClass http://www.eclipse.org/ocl/3.1.0/Pivot#//NamedElement"/>
    </eOperations>
    <eOperations name="addElements" lowerBound="1" eType="#//Environment">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="body" value="Environment{ elements = self.elements->includingAll(elements) }"/>
      </eAnnotations>
      <eParameters name="elements" lowerBound="1">
        <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/ocl/3.1.0/OCL.oclstdlib#//Collection">
          <eTypeArguments eClassifier="ecore:EClass http://www.eclipse.org/ocl/3.1.0/Pivot#//NamedElement"/>
        </eGenericType>
      </eParameters>
    </eOperations>
    <eOperations name="nestedEnv" lowerBound="1" eType="#//Environment">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="body" value="Environment{ elements = self.elements }"/>
      </eAnnotations>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" ordered="false"
        upperBound="-1" eType="ecore:EClass http://www.eclipse.org/ocl/3.1.0/Pivot#//NamedElement"/>
  </eClassifiers>
</ecore:EPackage>
