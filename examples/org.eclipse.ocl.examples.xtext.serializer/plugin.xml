<plugin
      name="%pluginName"
      id="org.eclipse.ocl.examples.xtext.serializer"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
      <import plugin="org.eclipse.emf.ecore.xmi" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Idioms -->
      <package
            uri="http://www.eclipse.org/ocl/2020/Idioms"
            class="org.eclipse.ocl.xtext.base.cs2text.idioms.IdiomsPackage"
            genModel="model/Idioms.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated Idioms -->
      <parser
            type="idioms"
            class="org.eclipse.ocl.xtext.base.cs2text.idioms.util.IdiomsResourceFactoryImpl"/>
   </extension>

</plugin>
