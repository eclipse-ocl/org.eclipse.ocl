    <eOperations name="isPrimeNumber1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="body" value="isPrimeNumber2(candidatePrime, 3, Sequence{2})"/>
